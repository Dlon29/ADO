public async Task<List<string>> GetStatus45SwiftMsgIdsAsync(CancellationToken ct)
{
    var results = new List<string>();

    using var conn = new SqlConnection(_connString);
    using var cmd = new SqlCommand(
        "dbo.p_GetSwiftAckMessages_Status45", conn)
    {
        CommandType = CommandType.StoredProcedure
    };

    await conn.OpenAsync(ct);

    using var reader = await cmd.ExecuteReaderAsync(ct);
    while (await reader.ReadAsync(ct))
    {
        results.Add(reader.GetString(0));
    }

    return results;
}



public async Task UpdateAllStatus45To46Async(CancellationToken ct)
{
    using var conn = new SqlConnection(_connString);
    using var cmd = new SqlCommand(
        "dbo.p_UpdateSwiftStatus45To46", conn)
    {
        CommandType = CommandType.StoredProcedure
    };

    await conn.OpenAsync(ct);
    await cmd.ExecuteNonQueryAsync(ct);
}
